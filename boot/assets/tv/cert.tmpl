{{define "cert"}}
<!DOCTYPE html>
<html lang="en" class="color-toggle-light" color-mode="light">
{{template "head"}}

<body itemscope="" itemtype="https://schema.org/WebPage">
{{template "svg-sprite"}}

<div class="wrapper dark-mode-dim">
    {{template "header"}}

    <main class="container flex flex-even">
        {{template "aside"}}
        <!--- main page --->
        <div class="gdoc-page">
            <!--- header nav --->
            <div class="gdoc-page__header flex flex-wrap justify-between" itemprop="breadcrumb">
                <div>
                    <svg class="icon gdoc_path hidden-mobile"><use xlink:href="#gdoc_path"></use></svg>
                    <ol class="breadcrumb" itemscope="" itemtype="https://schema.org/BreadcrumbList">
                        <li itemprop="itemListElement" itemscope="" itemtype="https://schema.org/ListItem">
                            <a itemscope="" itemtype="https://schema.org/WebPage" itemprop="item" itemid="/v1/rk/tv/application/" href="/v1/rk/tv/application/">
                                <span itemprop="name">Application</span>
                                <meta itemprop="position" content="1">
                            </a>
                        </li>
                        <li>/</li>
                        <li itemprop="itemListElement" itemscope="" itemtype="https://schema.org/ListItem">
                            <span itemprop="name">Cert</span>
                            <meta itemprop="position" content="1">
                        </li>
                    </ol>
                </div>
            </div>

            <!--- content --->
            <article class="gdoc-markdown">
                <!--- title --->
                <div class="gdoc-page__anchorwrap">
                    <h1 id="example"> Cert <a data-clipboard-text="https://geekdocs.de/shortcodes/tabs/#example" class="gdoc-page__anchor gdoc-page__anchor--right clip" aria-label="Anchor Example" href="#example">
                            <svg class="icon gdoc_link"><use xlink:href="#gdoc_link"></use></svg>
                        </a>
                    </h1>
                    <p>List of certificates loaded while bootstrapping rk application.</p>
                </div>
                <!--- content --->
                <div class="gdoc-markdown">
                    {{ range . }}
                    <div class="gdoc-page__anchorwrap">
                        <h2>{{ .EntryName }}</h2>
                    </div>
                    <blockquote class="gdoc-hint info">
                    <!--- entry name, type and description --->
                    <table>
                        <thead>
                        <tr>
                            <th>Entry Name</th>
                            <th>Entry Type</th>
                            <th>Description</th>
                        </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>{{ .EntryName }}</td>
                                <td>{{ .EntryType }}</td>
                                <td>{{ .EntryDescription }}</td>
                            </tr>
                        </tbody>
                    </table>
                    </blockquote>

                    <blockquote class="gdoc-hint ok">
                    <table>
                        <thead>
                        <tr>
                            <th>Retriever fields</th>
                            <th>Detail</th>
                        </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td><strong>Provider</strong></td>
                                <td>{{ .Provider }}</td>
                            </tr>
                            <tr>
                                <td><strong>Endpoint</strong></td>
                                <td>{{ .Endpoint }}</td>
                            </tr>
                            <tr>
                                <td><strong>Locale</strong></td>
                                <td>{{ .Locale }}</td>
                            </tr>
                            <tr>
                                <td><strong>ServerCertPath</strong></td>
                                <td>{{ .ServerCertPath }}</td>
                            </tr>
                            <tr>
                                <td><strong>ServerKeyPath</strong></td>
                                <td>{{ .ServerKeyPath }}</td>
                            </tr>
                            <tr>
                                <td><strong>ClientCertPath</strong></td>
                                <td>{{ .ClientCertPath }}</td>
                            </tr>
                            <tr>
                                <td><strong>ClientKeyPath</strong></td>
                                <td>{{ .ClientKeyPath }}</td>
                            </tr>
                        </tbody>
                    </table>
                    </blockquote>

                    <div class="gdoc-tabs">
                        <input type="radio" class="gdoc-tabs__control hidden" name="tabs-uniqueid" id="tabs-uniqueid-0" checked="checked">
                        <label for="tabs-uniqueid-0" class="gdoc-tabs__label"> Server Cert </label>
                        <div class="gdoc-markdown--nested gdoc-tabs__content">
                            <div class="highlight">
                                   <pre class="prettyprint" id="{{ .EntryName }}-server-cert">{{ .ServerCert }}</pre>
                            </div>
                        </div>
                        <input type="radio" class="gdoc-tabs__control hidden" name="tabs-uniqueid" id="tabs-uniqueid-1">
                        <label for="tabs-uniqueid-1" class="gdoc-tabs__label"> Client Cert </label>
                        <div class="gdoc-markdown--nested gdoc-tabs__content">
                            <div class="highlight">
                                <pre class="prettyprint" id="{{ .EntryName }}-client-cert">{{ .ClientCert }}</pre>
                            </div>
                        </div>
                    </div>
                    {{ end }}
                </div>
            </article>
        </div>
    </main>
    {{template "footer"}}
</div>
</body>
</html>
{{end}}